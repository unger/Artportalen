@model IEnumerable<Artportalen.Sample.Kustobsar.Models.KustobsarSighting>
@{
    Layout = null;
}<?xml version='1.0' encoding='ISO-8859-1' ?>
<obslistning>
@foreach (var sighting in Model)
{
<observation>
    <artid>@sighting.TaxonId</artid>
    <svensktnamn>@sighting.CommonName</svensktnamn>
    <engelsktnamn>@sighting.EnglishName</engelsktnamn>
    <vetnamn>@sighting.ScientificName</vetnamn>
    <unsure></unsure>
    <attribut>@sighting.Attribute</attribut>
    <antal>@sighting.Quantity</antal>
    <lokal>@sighting.Site</lokal>
    <lokalid>@sighting.SiteId</lokalid>
    <xkoord>@sighting.SiteXCoord</xkoord>
    <ykoord>@sighting.SiteYCoord</ykoord>
    <rrkkod>@sighting.RrkKod</rrkkod>
    <datum></datum>
    <startdat>@sighting.StartDate</startdat>
    <slutdat>@sighting.EndDate</slutdat>
    <starttid>@sighting.StartTime</starttid>
    <sluttid>@sighting.EndTime</sluttid>
    <personer>@sighting.SightingObservers</personer>
    <kommentar>@sighting.PublicComment</kommentar>
    <obsid>@sighting.SightingId</obsid>
    <obsida></obsida>
    <hasmedia>@sighting.HasMedia</hasmedia>
    <fyndtyp>3</fyndtyp>
    <regionalstatus>@sighting.RegionalStatus</regionalstatus>
    <rapportmall>@sighting.ReportTemplate</rapportmall>
    <kalla>formular</kalla>
</observation>
}
</obslistning>
